{
  "manifest_version": 2,
  "name": "Demuskify",
  "version": "1.0",
  "description": "Replace Twitter favicon and logo with your favorite emojis.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*", "*://*.x.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
